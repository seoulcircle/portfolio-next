{
  "packageManager": "pnpm@8.6.12",
  "name": "portfolio",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "pnpm --filter portfolio dev",
    "build": "pnpm --filter portfolio build",
    "lint": "pnpm --filter portfolio lint",
    "storybook": "storybook dev -p 6006 --config-dir .storybook",
    "transform:tokens": "ts-node scripts/transformTokens.ts",
    "build:tokens": "tsx tokens/build.mts",
    "build:css": "tsx tokens/build-css.mts",
    "build-storybook": "pnpm --filter components run build-storybook -- -c ./.storybook -o ./storybook-static"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@storybook/addon-a11y": "^8.6.12",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-interactions": "^8.6.12",
    "@storybook/addon-toolbars": "^8.6.12",
    "@storybook/cli": "^8.6.12",
    "@storybook/nextjs": "^8.6.12",
    "@storybook/react": "^8.6.12",
    "@tokens-studio/sd-transforms": "^1.3.0",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.2",
    "@types/webpack": "^5.28.5",
    "next": "15.2.4",
    "style-dictionary": "^4.3.3",
    "token-transformer": "^0.0.33",
    "ts-node": "10.9.2",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "tsx": "^4.19.3",
    "typescript": "^5.0.2",
    "webpack": "^5.99.5"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.3.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@storybook/react-webpack5": "^8.6.12",
    "framer-motion": "^12.6.5",
    "lucide-react": "^0.484.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "storybook": "^8.6.12"
  },
  "engines": {
    "node": "18.x"
  }
}
